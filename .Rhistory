gender <- function(names, map_names){
n <- nrow(names)
for(i in seq_len(n)){
k <- which(names[[i, 1]] == map_names$name)
if (length(k) != 0) {
if (length(k) > 1) {
k <- k[1]
}
names[[2]][i] <- map_names$gender[k]
} else {
names[[2]][i] <- 'NA'
}
}
colnames(names) <- c('names', 'gender')
return(names)
}
